{
	"name": "api-client",
	"description": "API personalizada com CRUD completo para Directus v11.12",
	"icon": "api",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint",
		"api",
		"crud"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^11.0.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.2"
	}
}
